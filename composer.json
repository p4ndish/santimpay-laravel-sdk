{
    "name": "p4ndish/laravel-santimpay",
    "description": "A Laravel package for Santim Pay payment gateway integration.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "P4ndish\\SantimPay\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Dagim",
            "email": "p4ndish.dev@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0 || ^12.0",
        "firebase/php-jwt": "^7.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "P4ndish\\SantimPay\\SantimPayServiceProvider"
            ],
            "aliases": {
                "SantimPay": "P4ndish\\SantimPay\\Facades\\SantimPay"
            }
        }
    }
}
